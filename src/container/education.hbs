
<div>
{{#with main}}
<main>
  <section>
    {{> title data="My Educationscl" big=true}}
    <ul>
     {{#each education}}
        <li>{{name}} 
          {{#unless isEnd}} (НЕ навчалась) {{/unless}}
        </li>
        {{/each}}
     </ul>
    </section>



    <section>
      {{> title data="My certificates"}}
      <ul>
        {{#each certificates}}
        <li>
          {{name}} ({{id}})
        </li>
        {{/each}}
      </ul>
    </section>
    {{/with}}
</main>
</div>




